package com.hackerrank;

public class Array02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[] = {1,2,3,4,5,6,7};
		int d = 2;
		int n = a.length;
		int temp,j;
		for (int i=0;i<d;i++){
			temp = a[i];
			for(j=i;j<n-1;j+=d){
				a[j]=a[j+d];
			}
			a[j] = temp;
		}

		for(int k:a)
			System.out.print(k+" ");
	}

}
