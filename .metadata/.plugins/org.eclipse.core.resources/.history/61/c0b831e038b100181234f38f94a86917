package com.hackerrank;

import java.util.Arrays;

public class Sorting02 {

	public static void main(String[] args) {

		int prices[]={1, 12, 5, 111, 200, 1000, 10}, k=50;
		int res=0, out=0;

		Arrays.sort(prices);
		for(int i=0;i<prices.length;i++){
			res+=prices[i];
			if(res>=k)
				break;
			out++;
		}
		System.out.println(out);
	}

}
